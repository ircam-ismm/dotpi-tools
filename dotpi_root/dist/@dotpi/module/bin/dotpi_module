#!/usr/bin/env node
import { program } from 'commander';

import { installCommandDefine } from '../src/install.js';
import { uninstallCommandDefine } from '../src/uninstall.js';
import { createConfigCommandDefine } from '../src/createConfig.js';

installCommandDefine({ program });
uninstallCommandDefine({ program });
createConfigCommandDefine({ program });

program.parse();

